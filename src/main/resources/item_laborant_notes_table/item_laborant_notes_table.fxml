<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.*?>
<TableView xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <columns>
    <TableColumn prefWidth="75.0" text="Дата">
      <cellValueFactory>
        <PropertyValueFactory property="date"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Время">
      <cellValueFactory>
        <PropertyValueFactory property="time"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Карта">
      <cellValueFactory>
        <PropertyValueFactory property="number"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Пациент">
      <cellValueFactory>
        <PropertyValueFactory property="patientName"/>
      </cellValueFactory>
    </TableColumn>
  </columns>
  <columnResizePolicy>
    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
  </columnResizePolicy>
</TableView>